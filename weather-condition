if (msg.payload.weather === "Clear"  ) {
msg.payload = msg.payload.description +"take your umbrella!."+ "humidity"+msg.payload.humidity+ "\n wind speed"+msg.payload.windspeed+"Km";
  
  
  return msg;
}
else 
if (msg.payload.weather === "Rainy") {

  msg.payload = "take your umbrella, the weather is going to be rainy!";
  return msg;
}
else 
if (msg.payload.weather === "sun") {
  msg.payload = "Dont forget your suglasses!";
  return msg;

}
else 
if (msg.payload.weather === "Clouds" && msg.payload.humidity>75 ) {
  msg.payload = "the weather is cloudy and humÄ±d";
  return msg;
}
else 
if (msg.payload.weather === "Clouds" && msg.payload.humidity<=70 ) {
  msg.payload = "the weather is cloudy";
  return msg;
}
else 
if (msg.payload.weather === "light rain") {
  msg.payload = "the weather is going to be rainy!";
  return msg;
}
else 
